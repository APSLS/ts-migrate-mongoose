{
  "name": "ts-migrate-mongoose",
  "version": "2.0.3",
  "description": "A node/typescript based migration framework for mongoose",
  "author": "Alex Eagle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ilovepixelart/ts-migrate-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/ilovepixelart/ts-migrate-mongoose/issues"
  },
  "homepage": "https://github.com/ilovepixelart/ts-migrate-mongoose#readme",
  "directories": {
    "examples": "examples"
  },
  "keywords": [
    "migrate",
    "migration",
    "mongoose",
    "schema",
    "mongodb",
    "db",
    "nosql",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "migrate": "dist/cjs/bin.js"
  },
  "main": "dist/cjs/migrator.js",
  "module": "dist/esm/migrator.js",
  "types": "dist/types/migrator.d.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types && scripts/prepublish.sh",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "package": "npm run build && npm pack",
    "migrate": "ts-node-dev ./src/bin.ts",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "coverage": "jest --config jest.config.js --detectOpenHandles --coverage && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^9.4.0",
    "dotenv": "^16.0.2",
    "inquirer": "^8.2.3",
    "mongoose": "^6.6.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.0",
    "@types/inquirer": "^8.2.3",
    "@types/jest": "<28.0.9",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.16",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "<28.0.9",
    "merge": "^2.1.1",
    "open-cli": "^7.0.1",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  }
}
