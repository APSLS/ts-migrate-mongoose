{
  "name": "ts-migrate-mongoose",
  "version": "2.4.0",
  "description": "A node/typescript based migration framework for mongoose",
  "author": "Alex Eagle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ilovepixelart/ts-migrate-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/ilovepixelart/ts-migrate-mongoose/issues"
  },
  "homepage": "https://github.com/ilovepixelart/ts-migrate-mongoose#readme",
  "directories": {
    "examples": "examples"
  },
  "keywords": [
    "migrate",
    "migration",
    "migrations",
    "mongoose",
    "mongodb",
    "mongo",
    "schema",
    "db",
    "nosql",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "migrate": "dist/cjs/bin.js"
  },
  "main": "dist/cjs/migrator.js",
  "module": "dist/esm/migrator.js",
  "types": "dist/types/migrator.d.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types && scripts/chmod.sh",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "package": "npm run build && npm pack",
    "migrate": "node -r @swc/register ./src/bin.ts",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "coverage": "jest --config jest.config.js --detectOpenHandles --coverage && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@swc/register": "0.1.10",
    "chalk": "4.1.2",
    "commander": "9.4.1",
    "dotenv": "16.0.3",
    "inquirer": "8.2.4",
    "mongoose": "6.6.7"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.1",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.10",
    "@swc/helpers": "0.4.12",
    "@swc/jest": "0.2.23",
    "@types/inquirer": "9.0.2",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.3",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.26.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.2.1",
    "merge": "2.1.1",
    "open-cli": "7.1.0",
    "typescript": "4.8.4"
  }
}
